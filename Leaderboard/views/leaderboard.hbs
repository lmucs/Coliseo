<div class="mdl-layout mdl-layout--fixed-header mdl-js-layout mdl-color--grey-400">
  <main class="mdl-layout__content">
    <div class="customContainer">
      <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
        <thead>
          <tr>
            <th class="mdl-data-table__cell--non-numeric">Username</th>
            <th>Score</th>
            <th>Rank</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td class="mdl-data-table__cell--non-numeric">Akrs</td>
            <td class="Score">2500</td>
            <td class="Rank">1</td>
          </tr>
          <tr>
            <td class="mdl-data-table__cell--non-numeric">Alex</td>
            <td class="Score">2280</td>
            <td class="Rank">2</td>
          </tr>
          <tr>
            <td class="mdl-data-table__cell--non-numeric">Albert</td>
            <td class="Score">1970</td>
            <td class=-"Rank">3</td>
          </tr>
          <!-- Template for any new inserted rows in table:
            <tr>
            <td class="mdl-data-table__cell--non-numeric">Username</td>
            <td class="Score">Score</td>
            <td class="Rank">Rank</td>
          </tr> -->
        </tbody>
      </table>
    </div>
  </main>
</div>
